{
  "name": "@pierre/shared-types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for Pierre frontend applications",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./social": {
      "types": "./src/social.ts",
      "import": "./src/social.ts"
    },
    "./api": {
      "types": "./src/api.ts",
      "import": "./src/api.ts"
    },
    "./coaches": {
      "types": "./src/coaches.ts",
      "import": "./src/coaches.ts"
    },
    "./auth": {
      "types": "./src/auth.ts",
      "import": "./src/auth.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "typescript": "~5.8.3"
  },
  "files": [
    "src"
  ],
  "private": true
}

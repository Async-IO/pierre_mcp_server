{
  "name": "pierre-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --port 8082",
    "tunnel": "../bin/start-tunnel.sh",
    "tunnel:stop": "pkill -f 'cloudflared tunnel' && echo 'Tunnel stopped' || echo 'No tunnel running'",
    "start:tunnel": "../bin/start-tunnel.sh --start-expo",
    "android": "expo run:android --port 8082",
    "ios": "expo run:ios --port 8082",
    "web": "expo start --web --port 8082",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:ios": "detox build --configuration ios.sim.debug",
    "build:android": "detox build --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "e2e:build": "detox build --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test": "detox test --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/drawer": "^7.7.10",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@tanstack/react-query": "^5.90.16",
    "axios": "^1.13.2",
    "expo": "~54.0.31",
    "expo-auth-session": "^7.0.10",
    "expo-build-properties": "^1.0.10",
    "expo-clipboard": "^8.0.8",
    "expo-haptics": "^15.0.8",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "^15.0.10",
    "firebase": "^12.7.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.3.2"
  },
  "overrides": {
    "markdown-it": "^14.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/detox": "^17.14.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "detox": "^20.46.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "^54.0.16",
    "typescript": "~5.9.2"
  },
  "private": true
}
